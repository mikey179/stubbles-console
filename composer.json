{
    "name": "stubbles/console",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "version": "2.0.0-beta3",
    "license": "BSD",
    "description": "Stubbles package to build command line apps.",
    "require": {
        "php": ">=5.3.0",
        "mikey179/vfsStream": "1.0.*",
        "stubbles/core": "2.0.*"
    },
    "recommend": {
        "stubbles/input": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "net\\stubbles\\console": "src/main/php",
            "org\\stubbles\\console": "src/main/php"
        }
    },
    "bin": ["bin/stubcli", "bin/createConsoleApp"],
    "scripts": {
        "post-install-cmd": "org\\stubbles\\scripts\\ComposerScripts::postInstall",
        "post-update-cmd": "org\\stubbles\\scripts\\ComposerScripts::postUpdate"
    }
}
